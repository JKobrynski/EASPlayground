{
  "name": "eastest",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "ios:build:dev": "eas build -p ios --profile development",
    "android:build:dev": " eas build -p android --profile development",
    "ios:build:demo": "eas build -p ios --profile demo",
    "android:build:demo": " eas build -p android --profile demo",
    "publish:dev": "expo publish --release-channel development",
    "publish:demo": "expo publish --release-channel demo"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "expo": "~45.0.0",
    "expo-dev-client": "~1.0.0",
    "expo-status-bar": "~1.3.0",
    "expo-updates": "~0.13.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-dotenv": "^3.3.1",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "typescript": "~4.3.5"
  },
  "private": true
}
